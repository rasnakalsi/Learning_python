import tkinter as tk 

def blink():
    global is_white
    c=0
    # if c==10:
    #     after_cancel(id)

    if is_white:
        color="black"
        c+=1
    else:
        color="white"
    
    is_white=not is_white
    frame["bg"]=color
    # if c==10:
    #     after_cancel(id)
    id=frame.after(2500,blink)
    if c==10:
        after_cancel(id)

is_white=True
window=tk.Tk()

frame=tk.Frame(window,width=200,height=100,bg="white")
frame.after(500,blink)
frame.pack()

window.mainloop()