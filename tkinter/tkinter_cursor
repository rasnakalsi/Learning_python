import tkinter as tk 

window=tk.Tk()

label_1=tk.Label(window,text="arrow",cursor="hand2")
label_1.pack()

label_2=tk.Label(window,text="heart",cursor="heart")
label_2.pack()

label_3=tk.Label(window,text="coffee_mug",cursor="coffee_mug")
label_3.pack()

frame_1=tk.Frame(window,height=30,width=100,bg="#55BF40",cursor="heart")
frame_1.pack()

window.mainloop()